{
  "cli": {
    "version": ">=14.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "FIREBASE_API_KEY": "$FIREBASE_API_KEY",
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "FIREBASE_API_KEY": "$FIREBASE_API_KEY",
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "env": {
        "FIREBASE_API_KEY": "$FIREBASE_API_KEY",
        "OPENAI_API_KEY": "$OPENAI_API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
